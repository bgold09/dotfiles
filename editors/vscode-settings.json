{
    "editor.fontFamily": "'Consolas NF', Consolas, monospace",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/bin": true,
        "**/obj": true,
        "**/objd": true,
        "**/.vs": true,
        "**/packages": true,
        "**/.packages": true,
        "**/pkg": true,
        "**/*.projhash": true,
        "**/*.userData": true,
        "**/*.assemblies": true,
        "**/.DS_Store": true, 
        "**/msbuild.log": true,
        "**/TypeScriptCompilerOutput.txt": true
    },
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\tools\\neovim\\Neovim\\bin\\nvim.exe",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.colorCustomizations": {
        "editorCursor.background": "#fdf6e3",
        "editorCursor.foreground": "#268bd2"
    },
    "workbench.iconTheme": "vs-seti",
    "workbench.commandPalette.preserveInput": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "window.zoomLevel": 0,
}

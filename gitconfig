	pager = less -r

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	sub = submodule
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	revert = checkout 
	d = diff --color-words

